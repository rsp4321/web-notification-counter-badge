{
  "name": "web-notification-counter-badge",
  "description": "Chrome extension that displays as badge the notification count of a web app",
  "version": "0.4",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab"],
  "action": {
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://info.cern.ch/hypertext/*",
        "https://web-push-book.gauntface.com/demos/notification-examples/*"
      ]
    }
  ]
}